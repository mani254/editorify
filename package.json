{
   "name": "editorify-dev",
   "version": "1.0.1",
   "main": "bundle.js",
   "description": "A versatile open-source package providing reusable UI components for backend systems and development, including a powerful text-to-HTML editor ,intuitive image uploader, calendar etc. Designed to simplify content management and enhance user interfaces for e-commerce platforms and other web applications.",
   "keywords": [
      "html-editor",
      "text-editor",
      "image-uploader",
      "calendar",
      "UI-components",
      "reusable-components",
      "backend-ui",
      "e-commerce",
      "content-management",
      "open-source",
      "opensource"
   ],
   "scripts": {
      "build": "webpack --mode production",
      "start": "webpack serve --mode development"
   },
   "exports": {
      ".": {
         "import": "./dist/index.js",
         "require": "./dist/index.js"
      },
      "./css": {
         "import": "./dist/styles.css",
         "require": "./dist/styles.css"
      },
      "./editor": {
         "import": "./dist/editor/editor.js",
         "require": "./dist/editor/editor.js"
      },
      "./editor/css": {
         "import": "./dist/editor/editor.css",
         "require": "./dist/editor/editor.css"
      },
      "./calendar": {
         "import": "./dist/calendar/calendar.js",
         "require": "./dist/calendar/calendar.js"
      },
      "./calendar/css": {
         "import": "./dist/calendar/calendar.css",
         "require": "./dist/calendar/calendar.css"
      }
   },
   "dependencies": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1"
   },
   "devDependencies": {
      "@babel/core": "^7.25.2",
      "@babel/preset-env": "^7.25.3",
      "@babel/preset-react": "^7.24.7",
      "babel-loader": "^9.1.3",
      "clean-webpack-plugin": "^4.0.0",
      "css-loader": "^7.1.2",
      "mini-css-extract-plugin": "^2.9.1",
      "webpack": "^5.93.0",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^5.0.4"
   },
   "peerDependencies": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1"
   },
   "author": "Sai Manikanta",
   "license": "ISC"
}